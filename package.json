{
  "name": "CW",
  "version": "1.0.0",
  "description": "Course Work",
  "scripts": {
    "start:prod": "NODE_ENV=production DEBUG=noti,info,resp,requ,errm forever ./api/bin/www",
    "start:staging": "NODE_ENV=staging DEBUG=noti,info,resp,requ,errm forever ./api/bin/www",
    "start:dev": "NODE_ENV=development DEBUG=noti,info,resp,requ,errm nodemon ./api/bin/www",
    "start": "DEBUG=noti,info,resp,requ,errm forever ./api/bin/www",
    "start-http": "NP_PROTOCOL=http DEBUG=noti,info,resp,requ,errm forever ./api/bin/www",
    "docs:develop": "browser-sync start -s 'api-docs' -f 'api-docs'",
    "test": "npm run test-unit & npm run test-integration",
    "test-unit": "cross-env NODE_ENV=test ./node_modules/.bin/mocha api/test/unit/*.test.js",
    "test-integration": "cross-env NODE_ENV=test ./node_modules/.bin/mocha api/test/integration/*.test.js"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "body-parser": "~1.17.1",
    "browser-sync": "^2.18.13",
    "cookie-parser": "~1.4.3",
    "date-format": "1.1.0",
    "debug": "2.6.8",
    "ejs": "~2.5.6",
    "express": "~4.15.2",
    "extend": "^3.0.1",
    "fb": "^2.0.0",
    "fs": "0.0.1-security",
    "js-yaml": "^3.9.1",
    "morgan": "~1.8.1",
    "pg": "^6.4.0",
    "pg-hstore": "^2.3.2",
    "request": "^2.83.0",
    "sequelize": "^4.3.1",
    "serve-favicon": "~2.4.2",
    "slack-node": "0.1.8"
  },
  "devDependencies": {
    "chai": "^4.1.0",
    "chai-http": "^3.0.0",
    "chai-things": "^0.2.0",
    "cross-env": "^5.0.5",
    "expect.js": "^0.3.1",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0"
  }
}
